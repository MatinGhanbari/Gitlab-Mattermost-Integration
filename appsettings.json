{
  
  "GITLAB_TOKEN": "[YOUR-GITLAB-TOKEN]",
  "hook_url": "[YOUR-HOOK-URL]",
  "gitlab_url": "[YOUR-GITLAB-URL]",
  "mattermost_profile_url": "https://[YOUR-MATTERMOST-HOST]/api/v4/users/{}/image",

  "bot_icon_url": "https://about.gitlab.com/images/press/logo/jpg/gitlab-logo-600.jpg",

  "use_mattermost_profile_icon": true,
  "summary_profile_pic_size": "25",
  "bot_username": "GitLabBot",
  "bot_channel": "gitlab",

  "service": {
    "host": "0.0.0.0",
    "port": 5010,
    "route": "/integrations/gitlab",
    "daily_scheduler": "18:30",
    "log_update": true,
    "card_info": false
  },
  
  "db_config": {
    "host": "[YOUR-DB-HOST]",
    "port": 5032,
    "dbname": "postgres",
    "user": "POSTGRESUSER",
    "password": "POSTGRESPASS"
  }
}